{
  "name": "@nsights/app",
  "version": "0.0.1",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-ts",
    "build-prod": "npm run build",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "start": "nodemon -r dotenv/config  dist",
    "start-dev": "npm run build-ts && node -r dotenv/config dist",
    "dev": "nodemon -r dotenv/config dist/index.js"
    
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "@types/supertest": "^2.0.7",
    "cross-env": "^5.2.0",
    "jest": "^24.5.0",
    "supertest": "^4.0.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.2",
    "@types/pg": "^7.4.14",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^1.18.10",
    "pg": "^7.9.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/lib/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
